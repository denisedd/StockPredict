    adOpVar = "http://pubads.g.doubleclick.net/gampad/ads?sz=3x3&iu=/4735792/" + iuUrl + "&ciu_szs=300x250,1500x692&cust_params=" + d + "&impl=s&gdfp_req=1&env=vp&output=xml_vast3&unviewed_position_start=1&correlator=" + f + "&url=" + a + "&description_url=" + pageURL;
    console.log("Video Ad Call:" + adOpVar);

    window.RTVEmbedder.init(player, {
        width: '100%',
        url: 'https://queso-cdn.prod.reuters.tv/index.html',
        resizable: false,
        mid: 'RCV006JCP',
        title: RTR_VideoTitle,
        hide_title: true,
        poster: 'https://s3.reutersmedia.net/resources/r/?d=20190401&i=RCV006JCP&w=652&r=RCV006JCP&t=2',
        suppress_ads: false,
        suppress_rtv: true,
        muted:  false,
        autoplay: false,
        preroll: true,
        allow_collapse: false,
        draggable: false,
        chartbeat_uid: 52639,
        ad_url: adOpVar
    });

    window.RTVEmbedder.addEventListener('ended', goToNextVideo);
    function goToNextVideo(type, params) {
        if (params.ima_playing !== 'true'){
            setTimeout(function(){
                location.replace('/video/2019/04/03/wall-street-turns-higher-on-trade-talk-h?videoId=533635679&videoChannel=5&channelName=Business');
            }, 500);
        }
    }

