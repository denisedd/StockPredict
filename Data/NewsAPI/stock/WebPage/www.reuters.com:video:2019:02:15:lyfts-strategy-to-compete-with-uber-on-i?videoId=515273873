var trackEventHandler = function(type, params) {
    videoQuartile = '';
    if (RCOM_map[type] == 'video_player.session.started'){
        videoType = 'START';
    } else if (RCOM_map[type] == 'video_player.session.finished'){
        videoType = 'CLOSER';
        videoQuartile = '1';
    } else if (RCOM_map[type] == 'video_player.opener.started' || RCOM_map[type] == 'video_player.opener.finished'){
        videoType = 'OPENER';
    } else if (RCOM_map[type] == 'video_player.story.quartile25'){
       RCOM_map[type] = 'video_player.story.quartile';
       videoQuartile = '.25';
    } else if (RCOM_map[type] == 'video_player.story.quartile50'){
       RCOM_map[type] = 'video_player.story.quartile';
       videoQuartile = '.50';
    } else if (RCOM_map[type] == 'video_player.story.quartile75'){
       RCOM_map[type] = 'video_player.story.quartile';
       videoQuartile = '.75';
    } else if (RCOM_map[type] == 'video_player.story.quartile100'){
       RCOM_map[type] = 'video_player.story.quartile';
       videoQuartile = '1';
    }  else if (RCOM_map[type] == 'video_player.ad.started' || RCOM_map[type] == 'video_player.ad.finished' || RCOM_map[type] == 'video_player.ad.skip'){
       videoType = 'PREROLL';
    }  else{ videoType = 'STORY'; }

    if (abp == false || abp == '0'){ adBlocked = '0'; } else { adBlocked = '1'; }

    

