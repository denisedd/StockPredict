        resizable: false,
        mid: 'OVA4VVX3V',
        title: RTR_VideoTitle,
        hide_title: true,
        poster: 'https://s4.reutersmedia.net/resources/r/?d=20190306&i=OVA4VVX3V&w=652&r=OVA4VVX3V&t=2',
        suppress_ads: false,
