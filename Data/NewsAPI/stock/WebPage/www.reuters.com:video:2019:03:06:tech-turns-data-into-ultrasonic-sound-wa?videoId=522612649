        platform: 'Desktop',
        content_type_channel: document.querySelector("meta[name='analyticsAttributes.channel']").content,
        content_channel: document.querySelector("meta[name='analyticsAttributes.contentChannel']").content,
        canonical_url: document.querySelector("meta[name='analyticsAttributes.canonicalUrl']").content,
        content_author: document.querySelector("meta[name='analyticsAttributes.author']").content,
        content_date: document.querySelector("meta[name='analyticsAttributes.articleDate']").content,
        ad_blocked: adBlocked,
        video_player_context: 'VOD',
        video_player_video_location: 'Video Page',
