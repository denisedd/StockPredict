        muted:  false,
        autoplay: false,
        preroll: true,
        allow_collapse: false,
        draggable: false,
        chartbeat_uid: 52639,
        ad_url: adOpVar
    });

    window.RTVEmbedder.addEventListener('ended', goToNextVideo);
    function goToNextVideo(type, params) {
        if (params.ima_playing !== 'true'){
            setTimeout(function(){
                location.replace('/video/2019/03/06/the-speed-bump-for-self-driving-cars?videoId=522527347&videoChannel=6&channelName=Technology');
            }, 500);
        }
    }

