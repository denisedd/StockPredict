			$(function(){  
				
				var ajax_request;
				var types_model = {"mls_20_ja":"1","mls_20_jc":"0"};
				var types_model_js = encodeURIComponent(JSON.stringify(types_model));
				var moduleid = 240;
				
				// Instance selector function
				jQuery("#instance-selector-mls_mod_240 .subinstance").on("click", function(event) {	
					
					jQuery("#mls-results-cont-mls_mod_240").removeClass("expanded");
					jQuery("#mls-results-mls_mod_240").empty();
					
					if (jQuery(this).find("i").hasClass("fa-check-square-o")) {
						var this_subinstance = jQuery(this).attr("id");
							types_model[this_subinstance] = "0";
							
						jQuery(this).find("i").removeClass("fa-check-square-o");
						jQuery(this).find("i").addClass("fa-square-o");
						jQuery(this).addClass("not-selected");
						
					} else {
						var this_subinstance = jQuery(this).attr("id");
							types_model[this_subinstance] = "1";
							
						jQuery(this).find("i").addClass("fa-check-square-o");
						jQuery(this).find("i").removeClass("fa-square-o");
						jQuery(this).removeClass("not-selected");
					}
					// Encode types
					types_model_js = encodeURIComponent(JSON.stringify(types_model));
				});
												
				// Search input function
				jQuery("#mod-mls-searchword-mls_mod_240").bind("input", function(e) { 
					var this_val = jQuery("#mod-mls-searchword-mls_mod_240").val();
					this_val = encodeURIComponent(this_val);
				
					// Close mls-results-cont if input is empty
					if ((jQuery.trim(this_val)).length == 0) {
						ajax_request.abort();
						jQuery("#close-results-mls_mod_240").hide();
						var container = jQuery("#mls-results-cont-mls_mod_240");
						if (!container.is(e.target)
							&& container.has(e.target).length === 0)
						{
							jQuery("#mls-results-cont-mls_mod_240").removeClass("expanded");
							jQuery("#mls-loading-mls_mod_240 .mls-module-spinner").hide();
						}
					}
			
					if ((jQuery.trim(this_val)).length > 1) {
						
						// Check if there is a pending ajax request
						if(typeof ajax_request !== "undefined")
							ajax_request.abort();
							
						// Spinner
						if (!jQuery("#mls-loading-mls_mod_240 .mls-module-spinner .spinner").length)
							createSpinner("#mls-loading-mls_mod_240 .mls-module-spinner");
						jQuery("#mls-loading-mls_mod_240 .mls-module-spinner").css("display", "inline-block");
						
						// Execute new ajax request
						ajax_request = jQuery.ajax({
							type: "POST",
							async: true,
							cache: false,
							url: "https://adventuremotorcycle.com/index.php?option=com_miniteklivesearch&task=search.getContent&mlsmodule=6&lang=en&word=" + this_val + "&moduleid=" + moduleid + "&itemid=906",
							data: "types=" + types_model_js,
							beforeSend: function() {
									jQuery("#mls-results-cont-mls_mod_240").removeClass("expanded");
									jQuery("#instance-selector-cont-mls_mod_240").removeClass("expanded");
									jQuery("#close-results-mls_mod_240").hide();
									chosen = "";
									jQuery("#mls-results-mls_mod_240 li").removeClass("selected");
							},
							success: function(msg) {
									jQuery("#mls-loading-mls_mod_240 .mls-module-spinner").hide();
									jQuery("#mls-loading-mls_mod_240 .mls-module-spinner .spinner").remove();
									jQuery("#close-results-mls_mod_240").show();
									if(msg.length > 2) {
										jQuery("#mls-results-cont-mls_mod_240").addClass("expanded");
										jQuery("#mls-results-mls_mod_240").html(msg);
									} else {
										jQuery("#mls-results-cont-mls_mod_240").removeClass("expanded");
										jQuery("#mls-results-mls_mod_240").empty();
									}
							}
						})	
						
					}
				
				});	
				// End input function
				
				// Search input mouseup
				jQuery("#mod-mls-searchword-mls_mod_240").bind("mouseup", function() {
					
					jQuery("#mls-tooltip-cont-mls_mod_240").addClass("expanded");
					jQuery("#instance-selector-cont-mls_mod_240").removeClass("expanded");
					
					var this_val = jQuery("#mod-mls-searchword-mls_mod_240").val();
					this_val = encodeURIComponent(this_val);
					
					// Cancel ajax on submit form
					jQuery(document).on("submit","#mls-form-mls_mod_240",function() {
						ajax_request.abort();
						jQuery("#mls-loading-mls_mod_240 .mls-module-spinner").hide();
						jQuery("#mls-loading-mls_mod_240 .mls-module-spinner .spinner").remove();
					});
										
					if ((jQuery.trim(this_val)).length > 1) {
						if( !jQuery("#mls-results-mls_mod_240").is(":empty") ) {
							jQuery("#mls-results-cont-mls_mod_240").addClass("expanded");	
						} else {
							
							// Check if there is a pending ajax request
							if(typeof ajax_request !== "undefined")
							ajax_request.abort();
							
							// Spinner
							if (!jQuery("#mls-loading-mls_mod_240 .mls-module-spinner .spinner").length)
								createSpinner("#mls-loading-mls_mod_240 .mls-module-spinner");
							jQuery("#mls-loading-mls_mod_240 .mls-module-spinner").css("display", "inline-block");
						
							// Execute new ajax request
							ajax_request = jQuery.ajax({
								type: "POST",
								async: true,
								cache: false,
								url: "https://adventuremotorcycle.com/index.php?option=com_miniteklivesearch&task=search.getContent&mlsmodule=6&lang=en&word=" + this_val + "&moduleid=" + moduleid + "&itemid=906",
								data: "types=" + types_model_js,
								beforeSend: function() {
										jQuery("#mls-results-cont-mls_mod_240").removeClass("expanded");
										jQuery("#instance-selector-cont-mls_mod_240").removeClass("expanded");
										jQuery("#close-results-mls_mod_240").hide();
										chosen = "";
										jQuery("#mls-results-mls_mod_240 li").removeClass("selected");
								},
								success: function(msg) {
										jQuery("#mls-loading-mls_mod_240 .mls-module-spinner").hide();
										jQuery("#mls-loading-mls_mod_240 .mls-module-spinner .spinner").remove();
										jQuery("#close-results-mls_mod_240").show();
										if(msg.length > 2) {
											jQuery("#mls-results-cont-mls_mod_240").addClass("expanded");
											jQuery("#mls-results-mls_mod_240").html(msg);
										} else {
											jQuery("#mls-results-cont-mls_mod_240").removeClass("expanded");
											jQuery("#mls-results-mls_mod_240").empty();
										}
								}
							})
								
						}
					}
				});
				
				// Close mls-results-cont on click outside
				jQuery(document).mouseup(function (e) {
					var container = jQuery(".mls-results-cont");
					if ( !container.is(e.target) && container.has(e.target).length === 0 
						&&  !jQuery("#mod-mls-searchword-mls_mod_240").is(e.target) )
					{
						jQuery(".mls-results-cont").removeClass("expanded");
						chosen = "";
						jQuery("#mls-results-mls_mod_240 li").removeClass("selected");
					}
				});
								
				// Close button function
				jQuery("#close-results-mls_mod_240").on("click", function(event) {	
					jQuery("#mod-mls-searchword-mls_mod_240").val("");
					jQuery(this).hide();
					jQuery("#instance-selector-cont-mls_mod_240").removeClass("expanded");
				});
				
				// Selector button function
				jQuery("#selector-button-mls_mod_240").on("click", function(event) {	
					jQuery("#instance-selector-cont-mls_mod_240").toggleClass("expanded");
				});
				
				// Close instance-selector-cont on click outside
				jQuery(document).mouseup(function (e) {
					var container = jQuery("#instance-selector-cont-mls_mod_240");
					if ( (!container.is(e.target) && container.has(e.target).length === 0)
						&& (!jQuery("#mls-parent-mls_mod_240").is(e.target) && jQuery("#mls-parent-mls_mod_240").has(e.target).length === 0)					
					   )
					{
						jQuery("#instance-selector-cont-mls_mod_240").removeClass("expanded");
					}
				});
				
				// Arrows navigation function
				var chosen = "";
				jQuery(document).keydown(function(e){ // 38-up, 40-down
					if (e.keyCode == 40) { 
						if(chosen === "") {
							chosen = 0;
						} else if((chosen+1) < jQuery("#mls-results-mls_mod_240 li").length) {
							chosen++; 
						}
						jQuery("#mls-results-mls_mod_240 li").removeClass("selected");
						jQuery("#mls-results-mls_mod_240 li:eq("+chosen+")").addClass("selected");
						return false;
					}
					if (e.keyCode == 38) { 
						if(chosen === "") {
							chosen = 0;
						} else if(chosen > 0) {
							chosen--;            
						}
						jQuery("#mls-results-mls_mod_240 li").removeClass("selected");
						jQuery("#mls-results-mls_mod_240 li:eq("+chosen+")").addClass("selected");
						return false;
					}
				});
				
				// Escape key
				jQuery(document).keyup(function(e) {
  					if (e.keyCode == 27) {
						jQuery("#mls-results-cont-mls_mod_240").removeClass("expanded");
						jQuery("#instance-selector-cont-mls_mod_240").removeClass("expanded");
						chosen = "";
						jQuery("#mls-results-mls_mod_240 li").removeClass("selected");
					}
				});
				
				// Enter key
				jQuery("#mls-form-mls_mod_240").keypress(function(e) {
					if(e.which == 13) {
						if (jQuery("#mls-results-mls_mod_240 li.selected")[0]){
							window.location.href = jQuery("#mls-results-mls_mod_240 li.selected a").attr("href");
							e.preventDefault();
    						return false;
						} else {
							return true;
						}
					}
				});
				
				// Blur input
				jQuery("#mod-mls-searchword-mls_mod_240").bind("blur", function() {
					jQuery("#mls-tooltip-cont-mls_mod_240").removeClass("expanded");
				});
				
				// Initialize spinner
				var createSpinner = function(divIdentifier)
				{ 
					var spinner_options = {
					  lines: 9,
					  length: 3,
					  width: 2,
					  radius: 3,
					  corners: 1,
					  rotate: 0,
					  direction: 1,
					  color: "#058ec4",
					  speed: 1,
					  trail: 52,
					  shadow: false,
					  hwaccel: false,
					  className: "spinner",
					  zIndex: 2e9,
					  top: "50%",
					  left: "50%"
					};
					$(divIdentifier).html(new Spinner(spinner_options).spin().el);
					
					return;
				}
	
			})
		})(jQuery);
		
