    <script type="text/javascript">
        (function(){
            var stylesheets = document.querySelector("#navi-backport-stylesheets");

            if (!stylesheets) {
                return;
            }

            stylesheets = JSON.parse(stylesheets.textContent);

            var docFrag;
            var deprecatedNodes = [];

            stylesheets.forEach(function(sheet) {
                sheet = document.querySelector("link[href$=\"" + sheet + "\"]");

                if (sheet && sheet.parentNode.tagName !== "HEAD") {
                    docFrag = docFrag || document.createDocumentFragment();

                    docFrag.appendChild(sheet.cloneNode());

                    deprecatedNodes.push(sheet);
                }
            });

            if (docFrag) {
                document.head.appendChild(docFrag);

                deprecatedNodes.forEach(function(sheet) {
                    sheet.parentNode.removeChild(sheet);
                });
            }
        }());
    </script>

