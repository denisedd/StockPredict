        ids.forEach(function(id) {
            window.pageBuilder.loadAsyncChain(id);
        });
    };

window.pageBuilder.loadAsync = window.pageBuilder.loadAsync || function(){
        window.pageBuilder.loadAsyncFeatures();
        window.pageBuilder.loadAsyncChains();
