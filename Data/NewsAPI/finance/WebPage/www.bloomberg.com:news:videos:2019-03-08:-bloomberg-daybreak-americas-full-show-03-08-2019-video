        var fs, st, lt, pf, f = window._fence || {};
        if (d.getElementById(id)) return f;
        fs = d.scripts[0];
        st = d.createElement(s);
        lt = d.createElement(l);
        pf = d.createElement(l);
        ft = d.createElement(l);
        pf.rel = "dns-prefetch"; pf.href = cdn;
        st.id = id; st.async = true; st.src = cdn + "/s3/fence/" + v + "/app.bundle.js";
