require(["jquery-1", "istats-1"], function ($, istats) {
  $(function () {
    
    var monitorScrolling = function (ev) {
      if (isElementInViewport(document.getElementById('ns_chart_leagueoneoperatinglosses'))) {
        istats.log(
          "idt-content",
          "chart-entered-view",
