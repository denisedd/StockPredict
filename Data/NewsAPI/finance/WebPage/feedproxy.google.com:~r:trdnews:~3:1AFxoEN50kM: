<script>
   var networkCode = 1015965;
   var topLevelAdUnit = "trd-ny";
   var s1 = "news-articles";
   var adUnit = topLevelAdUnit + "/" + s1;
   var slotName = "/" + networkCode + "/" + adUnit;
   var pgtype = "articles";
   var pid = slugLimited;
	var adsStart = (new Date()).getTime();
    function detectWidth() {
        return window.screen.width || window.innerWidth || window.document.documentElement.clientWidth || Math.min(window.innerWidth, window.document.documentElement.clientWidth) || window.innerWidth || window.document.documentElement.clientWidth || window.document.getElementsByTagName('body')[0].clientWidth;
    }

    var TIMEOUT = 2000;

    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];
    var pbjs = pbjs || {};
    pbjs.que = pbjs.que || [];
    var adUnits = adUnits || [];

    var device = "desktop";
    if(detectWidth() <= 1024) device = "tablet";
    if(detectWidth() < 768) device = "mobile";

    function initAdServer() {
        if (pbjs.initAdserverSet) return;
        (function() {
            var gads = document.createElement('script');
            gads.async = true;
            gads.type = 'text/javascript';
            var useSSL = 'https:' == document.location.protocol;
            gads.src = (useSSL ? 'https:' : 'http:') +
                '//www.googletagservices.com/tag/js/gpt.js';
            var node = document.getElementsByTagName('script')[0];
            node.parentNode.insertBefore(gads, node);
        })();
        pbjs.initAdserverSet = true;
    };
    pbjs.timeout = setTimeout(initAdServer, TIMEOUT);
    pbjs.timeStart = adsStart;
    pbjs.device = device;
    pbjs.topLevelAdUnit = topLevelAdUnit;
    pbjs.s1 = s1;
    pbjs.pgtype = pgtype;

    function doDisplay(code){ 
        for (var i =0; i <= adPositionsKeys.length -1; i++) {
            if(adUnits[i].code == code){
               googletag.cmd.push(function() { googletag.display(code); });
               return true; 
            }
        }
        return false;
    }

    adPositions = {
                          "outstream" :   { "divId" : "div-id-for-outstream", "desktop" : "OutOfPage", "tablet" : "OutOfPage",  "mobile" : "OutOfPage"},
                         /*   "skin" :        { "divId" : "div-id-for-skin", "desktop" : "OutOfPage", "tablet" : false,  "mobile" : false}, */
                            "top" :         { "divId" : "div-id-for-top-leaderboard", "desktop" : [[970,250],[970,90],[728,90]], "tablet" : [[728,90]],  "mobile" : [[320,50],[300,50]]},
                            "right1" :      { "divId" : "div-id-for-rectangle1", "desktop" : [[300,250],[300,600]], "tablet" : false,  "mobile" : false},
                            "right2" :      { "divId" : "div-id-for-rectangle2", "desktop" : [[300,250],[300,600]], "tablet" : false,  "mobile" : false},
                            
                            "bottom" :      { "divId" : "div-id-for-bottom", "desktop" : [[300,250],[550,480]], "tablet" : [[300,250],[550,480]],  "mobile" : [[300,250]]},
                            "footer" :      { "divId" : "div-id-for-footer", "desktop" : [[970,250],[970,90],[728,90]], "tablet" : [[728,90]],  "mobile" : [[320,50],[300,50]]},
                            "sticky" :      { "divId" : "div-id-for-sticky", "desktop" : false, "tablet" : false,  "mobile" : [[320,50],[300,50]]},
                          };

var adPositionsKeys = Object.keys(adPositions);
    if(adPositionsKeys.length > 0){
        for (var i =0; i <= adPositionsKeys.length -1; i++) {
            if(!adPositions[adPositionsKeys[i]][device]) continue;
            adUnits.push({
                pos: adPositionsKeys[i],
                ooo: (adPositions[adPositionsKeys[i]][device] == "OutOfPage") ? true : false, // true for OutOfPage, false for normal adunit
                size: adPositions[adPositionsKeys[i]][device],
                code: adPositions[adPositionsKeys[i]].divId
            });
        }
    }

    googletag.cmd.push(function() {
      if(adUnits){
        for (var i = 0, len = adUnits.length; i < len; i++) {
            if(adUnits[i].ooo){
                googletag.defineOutOfPageSlot(slotName, adUnits[i].code).addService(googletag.pubads()).setTargeting("pos", adUnits[i].pos);
            } else {
                googletag.defineSlot(slotName, adUnits[i].size, adUnits[i].code).addService(googletag.pubads()).setTargeting("pos", adUnits[i].pos);
            }
           
        }
      }
    });


    googletag.cmd.push(function() {
        // Header Bidding Targeting
        pbjs.que.push(function() {pbjs.setTargetingForGPTAsync();});

        googletag.pubads().setTargeting("s1",s1);
        googletag.pubads().setTargeting("pid",pid);
        googletag.pubads().setTargeting("pgtype",pgtype);
        googletag.pubads().setTargeting("category",category);
        googletag.pubads().setTargeting("breakpoint",breakpoint);
        if( test ) {googletag.pubads().setTargeting("test",test);};

        // Init DFP
        googletag.pubads().enableSingleRequest();
        googletag.pubads().collapseEmptyDivs();
        googletag.enableServices();
    });
