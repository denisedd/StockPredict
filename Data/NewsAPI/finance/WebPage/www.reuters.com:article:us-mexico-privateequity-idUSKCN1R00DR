var rllly_rnd_state = (function() {
    function flatten(obj, depth) {
        var result = [], typ = (typeof obj), prop;
        if (depth && typ == 'object') {
            for (prop in obj) {
                try { result.push(flatten(obj[prop], depth - 1)); } catch (e) {}
            }
        }
        return (result.length ? result : typ == 'string' ? obj : obj + '\0');
    }
    function toInt(s) {
        var a=0;for (var i=0; i<s.length; i++) { a+=s.charCodeAt(i);}
        return a
    }
    return toInt(flatten(window.screen,1).toString()+flatten(window.navigator).toString())+(new Date().getMilliseconds()*100);
})();


function RLLYnextInt() {
    rllly_rnd_state = (rllly_rnd_a * rllly_rnd_state + rllly_rnd_c) % rllly_rnd_m;
    return rllly_rnd_state;
}
function RLLYnextFloat() {
    return RLLYnextInt() / (rllly_rnd_m - 1);
}
function RLLYRandom() {
    return RLLYnextFloat();
}
