function RLLYivp(e) {
    if ( !e || 1 !== e.nodeType ) return false;
    var de = d.documentElement;
    var r = e.getBoundingClientRect();
    if(typeof(e.width) === "undefined"){
        var h = e.offsetHeight;
    } else {
        var h = e.height;
    }
    if (h == 0 || w == 0){
        console.log("Element of id \'" + e.id + "\' and class \'" + e.className + "\' has no size.");
        return false;
    }
    var h2 = r.top + (h / 2);
    var h3 = 0;
    if(w.innerHeight) h3 = w.innerHeight;
    else if(de && !isNaN(de.clientHeight) && !isNaN(de.clientWidth)) h3 = de.clientHeight;
    return (r && (0 <= h2 && h2 <= h3));
}

