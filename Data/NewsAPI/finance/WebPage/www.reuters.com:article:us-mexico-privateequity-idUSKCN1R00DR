        function RLLYPost(failedEvents, successfulEvents, url) {

    function serializeEvents(events) {
        var list = '';
        for (var i = 0; i < events.length; i++) {
            if (list != '') {
                list += ',';
            }
            list += '"' + events[i] + '"';
        }
        return list;
    }

    
    var xhr = new XMLHttpRequest();
    xhr.responseType = 'text';
    xhr.open("POST", url, true);
    xhr.setRequestHeader('Content-Type', 'text/plain');
    xhr.onreadystatechange = function () {
        if (xhr.readyState === 4) {
            if (xhr.status === 200) {
                var answer = JSON.parse(xhr.responseText);
                
                if (answer.privateMode) {
                    w.rlllynl = true;
                    w.rlllyd81etc.pm = true;
                }
            } else {
                w.rlllynl = false;
            }
            w.rlllyd81etc.pmd = true;
            RLLYSync();
        }
    }
    xhr.send('{"failedRequests":[' + serializeEvents(failedEvents) + '],"successfulRequests":[' + serializeEvents(successfulEvents)+']}');
}

function RLLYIndexOf(needle, haystack) {
    for (var i = 0; i < haystack.length; i++) {
        if (haystack[i] === needle) {
            return i;
        }
    }
    return -1;
}


function RLLYSetupNetworkListeners() {

    function eventListener(eventArray, e) {
        if (!e.target) {
            return;
        }
        var src = e.target.src || '';
        if ((src !== '') && (src.indexOf('chrome-extension://') == -1)) {
            var hostname = new URL(src).hostname || '';
            if (hostname !== '') {
                if (RLLYIndexOf(hostname, eventArray) < 0) {
                    eventArray.push(hostname);
                }
            }
        }
    }
    
    var errorEvents = new Array();
    var loadEvents = new Array();
    var errorListener = eventListener.bind(null, errorEvents);
    var loadListener = eventListener.bind(null, loadEvents);
    document.addEventListener ? document.addEventListener("error", errorListener, true) : document.attachEvent && document.attachEvent("error", errorListener);
    document.addEventListener ? document.addEventListener("load", loadListener, true) : document.attachEvent && document.attachEvent("load", loadListener);
    w.RLLYnwld = {
        error: {
            events: errorEvents,
            listener: errorListener
        },
        load: {
            events: loadEvents,
            listener: loadListener
        },
    };

        (function (proxied) {
        document.createElement = function () {
            var newElement = proxied.apply(this, arguments);
            newElement.addEventListener ? newElement.addEventListener("error", w.RLLYnwld.error.listener, true) : newElement.attachEvent && newElement.attachEvent("error", w.RLLYnwld.error.listener);
            newElement.addEventListener ? newElement.addEventListener("load", w.RLLYnwld.load.listener, true) : newElement.attachEvent && newElement.attachEvent("load", w.RLLYnwld.load.listener);
            return newElement;
        }
    })(document.createElement);
}
function RLLYMonitorNetworkData() {

    function processResults() {

        function cleanupResults() {
            for (var i = 0; i < w.RLLYnwld.load.events.length; i++) {
                var position = RLLYIndexOf(w.RLLYnwld.load.events[i], w.RLLYnwld.error.events);
                if (position > -1) {
                    w.RLLYnwld.error.events.splice(position, 1);
                }
            }
        }


        cleanupResults();
        
        if (w.RLLYnwld.error.events.length > 0 && w.RLLYnwld.load.events.length > 0 ) {
            RLLYPost(w.RLLYnwld.error.events, w.RLLYnwld.load.events, rlllyCy('22dwrqdwr1gh2uwv2uhvrxufhv2u2Bp@6<)g@53494444)w@;)l@63;4554<59)z@335)u@\\Z]MI'+w.rllly3efRnd+'',-3));
        }
        else {
            w.rlllyd81etc.pmd = true;
            RLLYSync();
        }
    }

    var totalRequests = 0, oldRequests = 0, totalFailedRequests = 0, totalSuccessfulRequests = 0, listeningIntervalMaxCount = 10,
        listeningIntervalCount = 0, intervalDuration = 500, noData = 0;

    var listeningInterval = setInterval(function () {
        totalFailedRequests = w.RLLYnwld.error.events.size;
        totalSuccessfulRequests =w.RLLYnwld.load.events.size;
        totalRequests = totalFailedRequests + totalSuccessfulRequests;
        if (listeningIntervalCount <= listeningIntervalMaxCount) {
            // there are intervals left, but we have no change in the number of requests since the last interval
            if (totalRequests - oldRequests === 0 && listeningIntervalCount > 0) {
                noData++;
            }
            if (noData>4) {
                clearInterval(listeningInterval);
                processResults();
                noData = 0;
            }
        }
        else {
            // no more cycles left
            clearInterval(listeningInterval);
            processResults();
        }
        oldRequests = totalRequests;
        listeningIntervalCount++;
    }, intervalDuration);
}

