                function RLLYge(selector, travel) {
    var p = RLLYqe(selector);
    if (!p) {
        return p;
    }
    if (travel != '') {
        for (var i=0; i < travel.length; i++) {
            if (travel.charAt(i) == '^') {
                p= p.parentNode;
            }
            if (travel.charAt(i) == '<') {
                p= p.previousElementSibling;
            }
            if (travel.charAt(i) == '>') {
                p= p.nextElementSibling;
            }
            if (travel.charAt(i) == 'y') {
                p= p.firstChild;
            }
        }
    }
    return p;
}    function RLLYCoord1(event, element)  {
    var rect = element.getBoundingClientRect();
    var x = event.clientX - rect.left;
    var y = event.clientY - rect.top;
    return {'x': x, 'y': y };
}

function RLLYpos(e) {
    var xP = 0;
    var yP = 0;
    while (e) {
        xP += (e.offsetLeft + e.clientLeft);
        yP += (e.offsetTop + e.clientTop);
        e = e.offsetParent;
    }
    return { x: xP, y: yP };
}
function RLLYCoord2(e, elm) {
    var xP = 0;
    var yP = 0;
    var ip = RLLYpos(elm);
    if (e.pageX || e.pageY) {
        xP = e.pageX;
        yP = e.pageY;
    }
    if (e.clientX || e.clientY) {
        xP = e.clientX + document.body.scrollLeft + document.documentElement.scrollLeft;
        yP = e.clientY + document.body.scrollTop + document.documentElement.scrollTop;
    }
    xP = xP - ip.x;
    yP = yP - ip.y;
    return {'x': xP, 'y': yP };
}

function RLLYcoord(e, elm) {
    if (elm.getBoundingClientRect) {
        return RLLYCoord1(e,elm);
    }
    else {
        return RLLYCoord2(e, elm);
    }
}

function RLLYcevent(obj, fn) {
    if (obj.addEventListener) {
        obj.addEventListener('click', fn, true);
    } else if (obj.attachEvent) {
        obj.attachEvent('onclick', function () {
            return fn.call(obj, window.event);
        });
    }
}    function RLLYFloor(val) {
    return parseInt(val.toString().split(".")[0]);
}

