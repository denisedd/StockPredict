    }    function RLLYme(ad, oldSelector, randomString) {
    ad.classList.add("rlllyHid");
    if(oldSelector === ''){
        return '';
    }
    if(oldSelector[0] === '.'){
        ad.classList.remove(oldSelector.slice(1));
        ad.classList.add(randomString);
        return '.' + randomString;
    }
    ad.id = randomString;
    return '#' + ad.id.trim();
}

function RLLYgrs(length) {
    var text = "";
    var possible = "CBADEFGHIJKLMNOPQRSTUVWXYZcbadefghijklmnopqrstuvwxyz";
    for( var i=0; i < length; i++ ){
        text += possible.charAt(RLLYFloor(RLLYRandom() * possible.length));
    }
    return text;
}

function RLLYrs(position, allWrappers, adPositionId, mutationRecord) {
    var oldClass = ( position.classList.item(0) ? '.' + position.classList.item(0) : '');
    var newClass = RLLYme(position, oldClass, RLLYgrs(6));
    var oldId = ( position.id.trim() ? '#' + position.id.trim() : '');
    var newId = RLLYme(position, oldId, RLLYgrs(6));
    var newCssText = '';
    var ruleCssText = '';
    if(w[caesarShift('fmdsqfEtqqf', -12)]){
        var sheet = w[caesarShift('fmdsqfEtqqf', -12)];
        for(var k = 0; k < sheet.cssRules.length ; k++){
            ruleCssText = sheet.cssRules[k].cssText;
            if(oldClass !== '' && ruleCssText.indexOf(oldClass) > -1){
                newCssText = ruleCssText.replace(oldClass, newClass);
            }else if(oldId !== '' && ruleCssText.indexOf(oldId) > -1){
                newCssText = ruleCssText.replace(oldId, newId);
            }
            if(newCssText !== ''){
                sheet.deleteRule(k--);
                sheet.insertRule(newCssText, sheet.cssRules.length);
                newCssText = '';
            }
        }
    }
    position.classList.remove('rlllyHid');

    if(adPositionId && position.scrollHeight === 0 && position.scrollWidth === 0){
        if(mutationRecord[adPositionId] && mutationRecord[adPositionId].hidden > 3){
            mutationRecord[adPositionId].ignore = true;
        }else if (mutationRecord[adPositionId]) {
            mutationRecord[adPositionId].hidden++;
        }
        else {
            mutationRecord[adPositionId] = {hidden: 1}
        }
    }

    for(var j = 0 ; j < allWrappers.length ; j++){
        if(position.parentNode === allWrappers[j]){
            RLLYrs(allWrappers[j], allWrappers);
        }
    }
}

function allowMutation(adPositionId, mutationRecord) {
    return !mutationRecord[adPositionId] || !mutationRecord[adPositionId].ignore;
}

