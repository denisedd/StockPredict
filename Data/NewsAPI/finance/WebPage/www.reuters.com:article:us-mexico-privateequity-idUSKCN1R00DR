function RLLYchars() {
    var out = '';
    for (var a=65; a<65+26; a++) { out += String.fromCharCode(a)}
    for (var a=97; a<97+26; a++) { out += String.fromCharCode(a)}
    return out;
}
function RLLYatob(input) {

    //var _keyStr = "CBADEFGHIJKLMNOPQRSTUVWXYZcbadefghijklmnopqrstuvwxyz0123456789+/=";
    var _keyStr = RLLYchars() + "0123456789+/=";

    var output = "";
    var chr1, chr2, chr3;
    var enc1, enc2, enc3, enc4;
    var i = 0;
    input = input.replace(/[^A-Za-z0-9\+\/\=]/g, "");
    while (i < input.length) {
        enc1 = _keyStr.indexOf(input.charAt(i++));
        enc2 = _keyStr.indexOf(input.charAt(i++));
        enc3 = _keyStr.indexOf(input.charAt(i++));
        enc4 = _keyStr.indexOf(input.charAt(i++));
        chr1 = (enc1 << 2) | (enc2 >> 4);
        chr2 = ((enc2 & 15) << 4) | (enc3 >> 2);
        chr3 = ((enc3 & 3) << 6) | enc4;
        output = output + String.fromCharCode(chr1);
        if (enc3 != 64) {
            output = output + String.fromCharCode(chr2);
        }
        if (enc4 != 64) {
            output = output + String.fromCharCode(chr3);
        }
    }
    return output;
}    
