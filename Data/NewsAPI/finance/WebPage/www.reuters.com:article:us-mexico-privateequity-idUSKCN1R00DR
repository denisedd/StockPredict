    function RLLYinject2(wrappersAndAds) {
        var wrap = [];
        RLLYinjectwrappers(wrappersAndAds, wrap);
        RLLYinjectads(wrappersAndAds, wrap);
        RLLYcleanup();
    }

    function RLLYapplyAdTextHtmlAfter(className) {
        function applyAdTextHtmlAfter(adContainer) {
            var defaultStyles = {
                "color": "#979797", "max-width": "0px", "width": "100%", "margin": "0 auto",
                "text-align": "right", "top": "2px", "position": "relative", "padding-bottom": "5px",
                "font": "bold 10px/10px 'Helvetica Neue Condensed', Helvetica, sans-serif"
            };
            function resizeAdMarker(adContainer) {
                var adElement = adContainer.previousElementSibling;
                if (!adElement || adElement.children.length === 0) return;
                var calculatedMaxWidth = adElement.children[0].getBoundingClientRect().width;
                Object.keys(defaultStyles).forEach(function(style) {
                    if (!adContainer.style.getPropertyValue(style)) {
                        adContainer.style.setProperty(style, defaultStyles[style]);
                    }
                });
                adContainer.style.setProperty("max-width", calculatedMaxWidth + "px");
            }
            if (adContainer.hasAttribute("class")) {
                adContainer.removeAttribute("class");
                resizeAdMarker(adContainer);
                if (!w.rlllyMk) {
                    w.rlllyMk = true;
                    window.addEventListener("resize", function(event) {
                        var adMarkers = RLLYqes('[style*="color: rgb(151, 151, 151)"][style*="max-width"]');
                        for (var i = 0; i < adMarkers.length; i++) {
                            resizeAdMarker(adMarkers[i]);
                        }
                    });
                }
            }
        }
        if (!className) {
            className = "adv";
        }
        var adContainers = d.getElementsByClassName(className);
        for (var a = 0; a < adContainers.length; a++) {
            applyAdTextHtmlAfter(adContainers[a]);
        }
    }

    function RLLYapplyCSSStyles(obj, cssApply) {
        if (!obj || !cssApply) return;
        var cssKeys = Object.keys(cssApply);
        for (var css = 0; css < cssKeys.length; css++) {
            if (Object.prototype.toString.call(cssApply[cssKeys[css]]) === "[object Array]") {
                obj.style.setProperty(cssKeys[css], cssApply[cssKeys[css]], "important");
            } else {
                obj.style.setProperty(cssKeys[css], cssApply[cssKeys[css]], "");
            }
        }
    }

    function RLLYapplyAdText(adElement) {
        if (!adElement || adElement.children.length === 0 || (adElement.id === "" && adElement.class === "")) return;
        var imageElement = adElement.children[0],
            adSelector = adElement.id ? "#" + adElement.id : "." + adElement.className.trim().replace(" ", "."),
            adSelectorName = adSelector.substr(1) + "_",
            selfBounding = adElement.getBoundingClientRect(),
            imageBounding = imageElement.getBoundingClientRect(),
            width = (imageBounding.x - selfBounding.x) + imageBounding.width,
            myAdLabel = RLLYqe(adSelector + "_");
        if (!myAdLabel) {
            var bothName = adSelectorName;
            myAdLabel = RLLYce("style");
            myAdLabel.id = bothName;
            myAdLabel.className = bothName;
            document.head.appendChild(myAdLabel);
            window.addEventListener("resize", function (event) {
                RLLYapplyAdText(this);
            }.bind(adElement));
        }
        if (myAdLabel.innerHTML.indexOf(width + "px") === -1) {
            myAdLabel.innerHTML = adSelector + ":after { width: " + width + "px; } ";
        }
    }

    function RLLYcssUpdate(jsonArray, position) {
        if (!Object.prototype.toString.call(jsonArray) === '[object Array]') return;
        for (var j = 0; j < jsonArray.length; j++) {
            if ((jsonArray[j].selector || jsonArray[j].selectorAll) && !jsonArray[j].skipSelectorCSS) {
                var queried = RLLYqes(jsonArray[j].selector || jsonArray[j].selectorAll);
                for (var q = 0; q < queried.length; q++) {
                    RLLYapplyCSSStyles(queried[q], jsonArray[j].cssApply);
                    if (jsonArray[j].selector) break; // run only once, if normal selector is present
                }
            }
            if (jsonArray[j].function && typeof jsonArray[j].function === "function") {
                jsonArray[j].function();
            }
            if (jsonArray[j].applyAdText && typeof RLLYapplyAdText === "function") {
                
                RLLYapplyAdText(position);
            }
        }
    }

