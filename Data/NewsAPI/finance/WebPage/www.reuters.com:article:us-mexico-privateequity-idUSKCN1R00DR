    function RLLYcc(oldSelector, newSelector) {
        function copyCSSSelector(oldSelector, newSelector) {
            var found = false;
            for (var s = 0; s < document.styleSheets.length; s++) {
                try {
                    var sheet = document.styleSheets[s];
                    var rules = sheet.cssRules ? sheet.cssRules : sheet.rules;
                    var newCSS = '';
                    if (rules !== null) {
                        for (var i = 0; i < rules.length; i++) {
                            var regEx = new RegExp(oldSelector + '(?![-_a-zA-Z0-9])', 'g');
                            if (rules[i].selectorText && rules[i].selectorText.match(regEx)) {
                                found = true;
                                var currentRule = rules[i];
                                var newSelectorText = currentRule.selectorText.replace(regEx, newSelector);
                                newCSS += newSelectorText + '{' + currentRule.style.cssText + '} ';
                            }
                        }
                    }
                    appendNewCss(newCSS);
                } catch (e) {
                }
            }
            return found;
        }
        function appendNewCss(newCSS) {
            if (newCSS.length > 0) {
                var headReference = document.head || document.getElementsByTagName('head')[0],
                        styleElement = document.createElement('style');
                styleElement.type = 'text/css';
                if (styleElement.styleSheet) {
                    styleElement.styleSheet.cssText = newCSS;
                }
                else {
                    styleElement.appendChild(document.createTextNode(newCSS));
                }
                headReference.appendChild(styleElement);
            }
        }
        function replaceCSSSelector(oldSelector, newSelector) {
            var oldClassNameOrId = oldSelector.substr(1);
            var newClassNameOrId = newSelector.substr(1);
            if (oldSelector.match(/^\./)) {
                var elms = document.querySelectorAll(oldSelector);
                for (var e = 0; e < elms.length; e++) {
                    elms[e].className = elms[e].className.replace(oldClassNameOrId, newClassNameOrId);
                }
            }
            else {
                document.getElementById(oldClassNameOrId).id = newClassNameOrId;
            }
        }
        if (copyCSSSelector(oldSelector, newSelector)) {
            
            replaceCSSSelector(oldSelector, newSelector);
        }
    }


            function RLLYRnd() {
        var r = RLLYRndCln();
        
        return rlllyCy(r,3);
    }
    function RLLYRndCln() {
        var c = '0123456789bcdefghijklmnopqrstuvwxyzBCDEFGHIJKLMNOPQRSTUVWXYZ';
        var l = RLLYMax(8,1+RLLYFloor((RLLYRandom() * 8)));
        var r = '';
        while (r.length < l) {
            r += c.charAt( RLLYFloor((RLLYRandom() * c.length)));
        }
        return r;
    }
    function RLLYce(n) {
    return d.createElement(n);
}

function RLLYih(e,c) {
    e.innerHTML = c;
}

function RLLYac(e,c) {
    e.appendChild(c);
}

function RLLYsb(e,i) {
    e.style.backgroundImage="url(" + i.src  +")";
}

function RLLYib(e,c) {
    e.parentElement.insertBefore(c,e);
}

function RLLYia(e,c) {
    e.parentElement.insertBefore(c,e.nextSibling);
}

function RLLYre(e) {
    if (e && e.parentElement) {
        e.parentElement.removeChild(e);
    }
}

function RLLYhc(e, c) {
    return new RegExp(' ' + c + ' ').test(' ' + e.className + ' ');
}

function RLLYsc(e,c) {
    e.className = c;
}

function RLLYrc(e, c) {
    var nc = ' ' + e.className.replace( /[\t\r\n]/g, ' ') + ' ';
    if (RLLYhc(e, c)) {
        while (nc.indexOf(' ' + c + ' ') >= 0 ) {
            nc = nc.replace(' ' + c + ' ', ' ');
        }
        e.className = nc.replace(/^\s+|\s+$/g, '');
        if (e.className == '') {
            e.removeAttribute("class");
        }
    }
}

