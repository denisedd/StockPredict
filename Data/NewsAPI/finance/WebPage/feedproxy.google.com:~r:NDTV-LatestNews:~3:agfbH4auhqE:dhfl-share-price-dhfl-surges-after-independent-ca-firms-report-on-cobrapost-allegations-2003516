$(document).ready(function(e) {
        $('.topnav_cont li').hover(function() {
            $('ul', this).stop().slideDown(200);
        }, function() {
            $('ul', this).stop().slideUp(200);
        });
		
		$("#h_sub_menu").click(function() {
			e.stopPropagation()
			$('#nav').toggleClass('more_active');
            $('#subnav').toggle();
            $('.ntopnav_wrap').toggleClass('noshadow');

        });
        
        var sticky_navigation_offset_top = $('.ntopnav_wrap').offset().top;
        var topOfOthDiv = -1;
        var get_height = $('.ntopnav_wrap').outerHeight(true);
        var sticky_navigation = function() {
            var scroll_top = $(window).scrollTop();
            if (scroll_top > sticky_navigation_offset_top) {
                $('.ntopnav_wrap').addClass('sticky');
                $('.ntopnav_wrap2').addClass('sticky');
                $('.ntopnav_wrap').css({
                    'position': 'fixed',
                    'top': 0,
                    'left': 0,
                    'z-index': 10001

                });

                $("#topcont").css("margin-top", get_height);

                $('.ntopnav_wrap .topnav_cont a').css({
                    'padding': '10px 8px'
                });
                $('.poweredby img').css({
                    'padding-top': '5px',
                    'height': '30px'
                });
                $('.ndtvlogo').css({
                    'display': 'block'
                });
                $('#subnav').css({
                    'position': 'fixed',
                    'top': '59px',
                    'left': 0,
                    'z-index': 10000
                });
                $('.budget15_moretopnav').each(function() {
                    this.style.setProperty('right', '6px', 'important');
                });


            } else {
                $('.ntopnav_wrap').css({
                    'position': 'static'
                });
                $('.ntopnav_wrap').removeClass('sticky');
                $('.ntopnav_wrap2').removeClass('sticky');
                $('.ndtvlogo').css({
                    'display': 'none'
                });

                $("#topcont").css("margin-top", 0);
                $('.ntopnav_wrap .newcont3').css({
                    'border-top': 'none',
                    'border-radius': '0'
                });
                $('.poweredby img').css({
                    'padding-top': '0',
                    'height': '39px'
                });
                $('.ntopnav_wrap .topnav_cont a').css({
                    'padding': '10px 10px'
                });
                $('#subnav').css({
                    'position': 'static',
                    'top': '',
                });
                $('.budget15_moretopnav').each(function() {
                    this.style.setProperty('right', '11px', 'important');
                });

            }
        };
        sticky_navigation();
        $(window).scroll(function() {
            sticky_navigation();
        });
    });
    jQuery(document).ready(function($) {
        var offset = 300,
            offset_opacity = 1200,
            scroll_top_duration = 700,
            $back_to_top = $('.cd-top');

        //hide or show the "back to top" link
        $(window).scroll(function() {
            ($(this).scrollTop() > offset) ? $back_to_top.addClass('cd-is-visible'): $back_to_top.removeClass('cd-is-visible cd-fade-out');
            if ($(this).scrollTop() > offset_opacity) {
                $back_to_top.addClass('cd-fade-out');
            }
        });

        //smooth scroll to top
        $back_to_top.on('click', function(event) {
            event.preventDefault();
            $('body,html').animate({
                scrollTop: 0,
            }, scroll_top_duration);
        });
	
    });
</script><div class="newcont"><div style="width:1260px;height:auto;margin:0;" id="adspanel"></div><div class="newcont1 "><div class="newcont2"><div id="pagepusher_adbox"></div><div class="ins_wid990" id="topcont"><div class="topnavprofit" ><div class="topmenu"></div></div></div><style>#searchbutton{width: 17px;right: 12px;height: 17px;display: block;background: url(https://www.ndtv.com/static/images/search_icon.png) right center no-repeat;position: absolute;right: 15px;top: 17px;border: none;text-indent: -1099999px;cursor: pointer;}.quick_links ul li {width: 125px;float: left;display: inline-block;margin-right: 10px;background: url(https://www.ndtv.com/static/images/list-icon.png) 0px 13px no-repeat;padding: 8px 0;padding-left: 16px;}.RHS_links {background-color: #f9f9f9;border: solid 1px #eaeaea;padding: 15px 0; box-sizing: border-box;margin-bottom: 20px;}.RHS_links ul li {display: inline-block;padding-left: 10px;padding-right: 10px;border-right: solid 1px #0d4d79;}.RHS_links ul li:last-child {border: none;padding-right: 0px;}.RHS_links ul li a {color: #0d4d79;font-family: 'Roboto', sans-serif;font-size: 13px !important;font-weight: 700 !important;}</style><div class="ins_lftcont640 clr" id="newsDescriptionContainer"><div class="error_404_cont"><h1>Oops! There was a problem!</h1><h5>Sorry, but we can't find what you were looking for right now.</h5></div><div class="search_bar"><form id="topic_search_form" action="https://www.ndtv.com/topic/" name="searchform" method="get" onsubmit="return submit_topic_form();"><input id="s" type="text" value="" placeholder="Enter Search Term Here" name="q" autocomplete="off" style=""><a id="searchbutton" onclick="return submit_topic_form();" href="#"></a></form></div><div class="clear_fix"></div><div class="lft_full_container"><h4><span>Quick Links</span></h4><div class="quick_links"><ul><li><a href="https://www.ndtv.com/live/channel/ndtv24x7">Live TV</a></li><li><a href="https://www.ndtv.com/video">Videos</a></li><li><a href="http