                    data-shares="facebook|,twitter|"
                    data-url="https://www.nytimes.com/video/business/100000003182047/morgan-stanley-on-earnings.html?action=click&amp;contentCollection=business&amp;module=blog-video&amp;region=caption&amp;pgtype=article"
